<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>eu.unifiedviews</groupId>
        <artifactId>uv-pom-dpu</artifactId>
        <version>1.3.0-SNAPSHOT</version>
    </parent>
    <groupId>eu.unifiedviews.plugins</groupId>
    <artifactId>uv-l-filesToVirtuoso</artifactId>
    <name>L-FilesToVirtuoso</name>
    <description>VirtuosoLoader issues Virtuoso internal functions to load directory of RDF data.</description>
    <version>1.6.1-SNAPSHOT</version>
    <packaging>bundle</packaging>

    <repositories>
        <repository>
            <id>maven.eea.sk</id>
            <name>maven.eea.sk-public</name>
            <url>http://maven.eea.sk/artifactory/public/</url>
        </repository>
    </repositories>

    <properties>
        <dpu.package>eu.unifiedviews.plugins.loader.filestovirtuoso</dpu.package>
        <dpu.mainClass>VirtuosoLoader</dpu.mainClass>
    </properties>

    <dependencies>
        <!-- Virtuoso. -->
        <dependency>
            <groupId>eu.comsode</groupId>
            <artifactId>com.openlinksw.virtuoso.virt_sesame2</artifactId>
            <version>1.21-7-develop-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>eu.comsode</groupId>
            <artifactId>com.openlinksw.virtuoso.virtjdbc4_1</artifactId>
            <version>3.76-7-develop-SNAPSHOT</version>
        </dependency>
        <!-- Provided by Core. -->
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.0</version>
            <scope>provided</scope>
        </dependency>
        <!-- UnifiedViews helpers. -->
        <dependency>
            <groupId>eu.unifiedviews</groupId>
            <artifactId>uv-dataunit-helpers</artifactId>
            <version>[1.3.0-SNAPSHOT,2.0.0)</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>eu.unifiedviews</groupId>
            <artifactId>uv-dpu-helpers</artifactId>
            <version>[1.0.0-SNAPSHOT,2.0.0)</version>
            <scope>compile</scope>
        </dependency>
    </dependencies>

</project>
